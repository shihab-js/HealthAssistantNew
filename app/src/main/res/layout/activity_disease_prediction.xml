<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="shihab.diu.com.healthassistantnew.DiseasePrediction">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Head"
        android:id="@+id/btn_head_disease_prediction"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Eye"
        android:id="@+id/btn_eye_disease_prediction"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Stomach"
        android:id="@+id/btn_stomach_disease_prediction"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Teeth"
        android:id="@+id/btn_teeth_disease_prediction"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nose"
        android:id="@+id/btn_nose_disease_prediction"
        />


</LinearLayout>


